<?xml version="1.0"?>
<launch>
  <!--
    Unified MPCC Follower Launch File
    支持全向底盘的前进/倒车/原地旋转统一控制

    使用方法：
    roslaunch local_planner mpccfollower_unified.launch
  -->

  <!-- MPCC Follower Node -->
  <node pkg="local_planner" type="mpccfollower_unified" name="mpccfollower" output="screen">
    <rosparam file="$(find local_planner)/config/mpccfollower_unified.yaml" command="load"/>
  </node>

  <!-- MPCC Prediction Visualizer -->
  <node pkg="local_planner" type="mpcc_pred_visualizer.py" name="mpcc_pred_visualizer" output="screen"/>

</launch>
